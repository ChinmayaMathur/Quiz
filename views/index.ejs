<%- include("./partials/header") %>

<h1>All Clucks</h1> 
<% for (let cluck of clucks) { %>
      <div class="card" style="width: 18erm;">
            <div class="card-body">
                  <img src="https://media.istockphoto.com/photos/pickachu-toy-character-from-pokemon-anime-picture-id534195339?b=1&k=6&m=534195339&s=170x170&h=f70B15DpFfROnE5B4tJOCItFn_M3OQpuoVuWRoFZqGk=" alt="pikachu" class="img">
                  <h5 class="class-title"><%= username %></h5>
                  <p class="card-text"><%= cluck.createdAt %> </p>
                  <p class="card-text"><%= cluck.content %></p>
            </div>
            <img src="<%= cluck.img_url %>" alt="image should be here" class="imgTop">
      </div>
<% } %>

<%- include("./partials/footer") %>
